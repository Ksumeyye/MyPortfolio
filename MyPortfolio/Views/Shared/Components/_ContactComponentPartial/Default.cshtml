@model List<MyPortfolio.DAL.Entities.Contact>
<section id="contact" @* class="s-contact-target-section" *@>

    <div class="overlay"></div>
    <br/>
    <div class="row narrow section-intro">
        <div class="col-full">
            <br/>
            <br/>
            <h3>İletişim</h3>
            <h1>Bana Ulaşın...</h1>
        </div>
    </div>
    @await Component.InvokeAsync("_MessageComponentPartial")
    <div class="row contact__main">

        @foreach (var item in Model)
        {
        <div class="col-four tab-full contact__infos">
            <h4 class="h06">Telefon</h4>
            <p>
                Telefon 1: @item.Phone1<br>
                Telefon 2: @item.Phone2<br>
            </p>

            <h4 class="h06">E-mail</h4>
            <p>
               @item.Email1<br>
               @item.Email2
            </p>

            <h4 class="h06">Adres</h4>
            <p>
                @item.Description<br>
                @item.Title
                @item.Address<br>

            </p>
        </div>
        }

    </div>
            
</section>