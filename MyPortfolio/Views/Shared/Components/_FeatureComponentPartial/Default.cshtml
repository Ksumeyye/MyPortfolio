@model List<MyPortfolio.DAL.Entities.Feature>
<section id="home" class="s-home page-hero target-section" data-parallax="scroll" data-image-src="https://r.resimlink.com/zU1T26Q.jpg" data-natural-width=3000 data-natural-height=2000 data-position-y=center>

    <div class="overlay"></div>
    <div class="shadow-overlay"></div>
