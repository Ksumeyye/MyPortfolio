                    <a href="/Default/Index/#works" class="smoothscroll btn btn--stroke">
                        Son Projelerim
                    </a>
                    <a href="/Default/Index/#about" class="smoothscroll btn btn--stroke">
                        Hakkımda
                    </a>
                </div>

                <div class="home-content__scroll">
                    <a href="#about" class="scroll-link smoothscroll">
                        <span>Aşağı İn</span>
                    </a>
                </div>

            </div>

        </div> <!-- end home-content -->
    }
    

